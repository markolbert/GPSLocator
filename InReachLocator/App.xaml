<Application
    x:Class="J4JSoftware.InReach.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:J4JSoftware.InReach"
    xmlns:sfcore="using:Syncfusion.UI.Xaml.Core"
    xmlns:map="using:MapControl">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
                <!--<ResourceDictionary Source="ui/DefaultResources.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->

            <local:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter"/>
            <local:EmptyCollectionToVisibilityConverter x:Key="EmptyCollectionToVisibilityConverter"/>
            <sfcore:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>

        </ResourceDictionary>
    </Application.Resources>
</Application>
